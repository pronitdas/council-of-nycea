{
  "name": "uaip-backend",
  "version": "1.0.0",
  "description": "Unified Agent Intelligence Platform Backend Services",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "clean": "pnpm exec tsc -b tsconfig.build.json --clean && pnpm exec tsc -b tsconfig.build_shared.json --clean",
    "build-services": "pnpm exec tsc -b tsconfig.build.json",
    "build-shared": "pnpm exec tsc -b tsconfig.build_shared.json ",
    "dev": "pnpm -r run dev",
    "lint": "pnpm -r exec eslint . --ext .ts"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "concurrently": "^8.2.2",
    "esbuild": "^0.19.8",
    "eslint": "^8.56.0",
    "glob": "^10.3.10",
    "rimraf": "^5.0.5",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "license": "MIT",
  "pnpm": {
    "onlyBuiltDependencies": [
      "bcrypt",
      "esbuild",
      "msgpackr-extract"
    ]
  }
}