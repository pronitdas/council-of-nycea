{
  "name": "@uaip/agent-intelligence",
  "version": "1.0.0",
  "description": "UAIP Agent Intelligence Engine - Context analysis and decision making",
  "main": "./dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon --exec tsx src/index.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "db:migrate": "tsx src/database/migrate.ts",
    "db:seed": "tsx src/database/seed.ts"
  },
  "dependencies": {
    "@uaip/types": "workspace:*",
    "@uaip/shared-services": "workspace:*",
    "@uaip/utils": "workspace:*",
    "@uaip/config": "workspace:*",
    "@uaip/middleware": "workspace:*",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "node-cron": "^3.0.3",
    "joi": "^17.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "typescript": "^5.3.3",
    "tsx": "^4.6.2",
    "nodemon": "^3.0.2",
    "vitest": "^1.1.0",
    "tsc-alias": "^1.8.8"
  }
} 